import java.util.*;
import java.lang.*;
import java.io.*;

class Str_dsa
{
	public static void main (String[] args) throws java.lang.Exception
	{
		String s = "carrotcake";
		int n = s.length();
		
		for(int i=0;i<n;i++){
		    for(int j=i+1;j<=n;j++){
		        System.out.println(s.substring(i,j));
		    }
		}
		System.out.println(" ");

	}
}
